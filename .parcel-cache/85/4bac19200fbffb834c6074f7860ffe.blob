      <section class="section signup--sec hidden" id="signup">
      <div class="bg-img-1 " id="SIGNUP">
        <button class="home--btn" type="submit"><a class="anc" href="#HEAD"></a><i class="fa fa-home"></i>Home</button>
        <div class="FORM-DIV" id="signup">
          <header class="form-header">SIGN-UP FORM</header>

          <form action="#" class="sign-up">
            <div class="user-details info">
              <i class="fa fa-user fa-2x" aria-hidden="true"></i>
              <label for="username" class="label">
                username
                <input type="text" name="username" id="username" class="signup-user">
              </label>
              <label for="pwd " class="pass label">Password
                <input type="text" name="email" id="password" class="signup-password">
              </label>
            </div>
            <div class="facebook-details info">
              <i class="fa fa-facebook-official fa-2x" aria-hidden="true"></i>
              <label for="Facebook" class="label">Facebook User Name
                <input type="text" name="Facebook" id="fb-username" class="fb-user">
              </label>
              <label for="Fb-pass " class="pass label">Facebook Password
                <input type="pwd" name="Facebook" id="fb-password" class="fb-pass">
              </label>
            </div>
            <div class="Instagram-details info">
              <i class="fa fa-instagram fa-2x" aria-hidden="true"></i>
              <label for="Instagram" class="label">Instagram User Name
                <input type="text" name="Instagram" id="ins-username" class="insta-user">
              </label>
              <label for="Ins-password " class="pass label">Instagram Password
                <input type="pwd" name="Instagram" id="ins-password" class="insta-pass">
              </label>
            </div>
            <div class="Gmail-details info">
              <i class="fa fa-envelope fa-2x" aria-hidden="true"></i>
              <label for="Gmail" class="label">Twitter User Name
                <input type="text" name="Gmail" id="Gmail-username" class="twit-user">
              </label>
              <label for="Gmail-pasword " class="pass label">Twitter Password
                <input type="pwd" name="Gmail" id="Gmail-password" class="twit-pass">
              </label>
            </div>
            <div class="Twitter-details info">
              <i class="fa fa-twitter-square fa-2x" aria-hidden="true"></i>
              <label for="Twitter" class="label">Gmail User Name
                <input type="text" name="Twitter" id="Twitter-username" class="gmail-user">
              </label>
              <label for="Twitter-password " class="pass label">Gmail Password
                <input type="pwd" name="Twitter" id="Twitter-password" class="gmail-pass">
              </label>
            </div>
            <div class="GitHub-details info">
              <i class="fa fa-github fa-2x" aria-hidden="true"></i>
              <label for="GitHub" class="label">GitHub User Name
                <input type="text" name="GitHub" id="GitHub-username" class="git-user">
              </label>
              <label for="GitHub-password " class="pass label">GitHub Password
                <input type="pwd" name="GitHub" id="GitHub-password" class="git-pass">
              </label>
            </div>

            <button class="submit-btn btn-primary-outline text-white">
              <a href="HEAD" class="link-color"></a> SIGN-UP
            </button>
          </form>
        </div>
