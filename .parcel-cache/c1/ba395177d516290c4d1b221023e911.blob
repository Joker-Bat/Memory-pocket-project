.form-container {
  box-sizing: border-box;
  margin: 0;
  height: 100%;
  width: 100%;
  overflow: hidden;
  font-family: "Lato", sans-serif;
  font-weight: 700;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #555;
  background-color: #ecf0f3;
}
.login-div {
  position: relative;
  bottom: 40px;
  margin-top: 5%;

  width: 430px;
  height: 550px;
  padding: 60px 35px 35px 35px;
  border-radius: 40px;
  background-color: #ecf0f3;
  box-shadow: 13px 13px 20px #cbced1, -13px -13px 20px #fff;
}
.logo {
  position: relative;
  bottom: 20px;
  color: #4da6ff;
  font-size: 80px;
  margin-left: 40%;
  margin-bottom: 20%;
  font-family: "Abel", sans-serif;
  font-family: "Karantina", cursive;
  display: inline;
  border-radius: 20%;
  box-shadow: 0px 0px 2px #cecccc, 0px 0px 0px 5px #ecf0f3, 8px 8px 15px #a7aaaf,
    -8px -8px 15px #fff;
}
.title {
  text-align: center;
  font-size: 28px;
  letter-spacing: 0.5px;
  font-weight: bolder;
}
.fields {
  width: 100%;
  padding: 40px 5px 5px 5px;
}
.fields input {
  border: none;
  background: none;
  outline: none;
  font-size: 18px;
  color: #555;
  padding: 10px 10px 20px 5px;
}
.username--1,
.password--1 {
  margin-right: 30px;

  border-radius: 25px;
  box-shadow: inset 8px 8px 8px #cbced1, inset -8px -8px 8px #fff;
}
.password--1 {
  margin-top: 10px;
}
.fields svg {
  height: 22px;
  margin: 0px 10px -3px 25px;
}
.signin--btn {
  outline: none;
  border: none;
  cursor: pointer;
  width: 90%;
  height: 60px;
  font-weight: bolder;
  color: #ecf0f3;
  background-color: #02c8db;
  border-radius: 25px;
  margin-top: 20px;
  box-shadow: 3px 3px 8px #b1b1b1, -3px -3px 8px #fff;
  transition: all 0.5s;
}
.signin--btn:hover {
  background-color: #50e5f9;
}
.signin--btn:active {
  background-color: #88ef9e;
}
.link--tag {
  padding-top: 20px;
  text-align: center;
}
.link--tag a {
  text-decoration: none;
  font-size: 15px;
}
/* sigup form css */
.bg-img-1 {
  background: url("black-1.HASH_REF_0a3391aa06e7c24ff718ade22be861be.jpeg");
  height: 100vh;
  background-size: cover;
  background-position: center;
  background-attachment: fixed;
}
.home--btn {
  margin-left: 80%;
  margin-top: 10px;
  border-radius: 10px;
}
.FORM-DIV {
  position: relative;
  top: 50%;
  left: 50%;

  z-index: 999;
  text-align: center;
  transition: transform 1s, opacity 1s;

  width: 600px;
  height: 500px;
  transform: translate(-50%, -50%);
  background: rgba(255, 255, 255, 0.04);
  box-shadow: -1px 4px 28px 0px rgba(0, 0, 0, 0.75);
}
.FORM-DIV header {
  color: white;
  font-size: 33px;
  font-weight: 600;
  margin: 0 0 35px 0;
  font-family: "Montserrat", sans-serif;
}
.info {
  display: flex;
}
.FORM-DIV label {
  color: #e0ebeb;
  font-family: "Montserrat", sans-serif;
  font-weight: 600;
}
.FORM-DIV input {
  border: none;
  outline: none;
  color: #222;
  font-size: 16px;
  font-family: "Poppins", sans-serif;
  border-radius: 5px;
}
.FORM-DIV i {
  position: relative;
  top: 20px;
  left: 20px;
}
.FORM-DIV button {
  position: relative;
  top: 20px;
  width: 80%;
  background-color: rgb(0, 110, 255);
}
.FORM-DIV button:hover {
  background-color: rgb(7, 98, 218);
  list-style: none;
}
.section--sign-up {
  background-color: #37383d;
  border-top: none;
  border-bottom: 1px solid #444;
  text-align: center;
  padding: 10rem 3rem;
}
.section--sign-up .section__header {
  color: #fff;
  text-align: center;
  font-size: 2rem;
  line-height: 1.3;
  font-weight: 400;
}
.section--sign-up .section__title {
  margin-bottom: 6rem;
}
.section--sign-up .btn {
  font-size: 1.9rem;
  padding: 2rem 5rem;
  display: inline-block;
  background-color: #5ec576;
  font-size: 1.6rem;
  font-family: inherit;
  font-weight: 500;
  border: none;
  padding: 1.25rem 4.5rem;
  border-radius: 10rem;
  cursor: pointer;
  transition: all 0.3s;
}
/* FOOTER */
.footer {
  padding: 3rem 3rem;
  background-color: #37383d;
}
.footer__nav {
  list-style: none;
  display: flex;
  justify-content: center;
  margin-bottom: 5rem;
}
.footer__item {
  margin-right: 4rem;
}
.footer__link {
  font-size: 1.6rem;
  color: #eee;
  text-decoration: none;
}
.footer__logo {
  height: 5rem;
  display: block;
  margin: 0 auto;
  margin-bottom: 5rem;
}
.footer__copyright {
  font-size: 1.4rem;
  color: #aaa;
  text-align: center;
}
.footer__copyright .footer__link {
  font-size: 1.4rem;
}
/* details page */
.details {
  background-color: #f3f3f3;
}
#Details {
  font-family: "Orelega One", cursive;
}
#Details h1 {
  font-size: 4rem;
  margin-left: 15%;
  margin-top: 5%;
}
.info--1 {
  margin-top: 20px;
  margin-left: 15%;
}
.user {
  margin-top: 3%;
  margin-left: 15%;
}
.paint {
  background-color: #ffcc66;
  border-radius: 5px;
  width: 20px;
}
.name {
  margin-left: 22%;
}
.user--saved {
  margin-top: 2%;
  margin-left: 13%;
}
.user--saved .detail--paint {
  background-color: #fcfcfc;
  border-radius: 20px;
}
.user--saved .left {
  position: relative;
  left: 30px;
}
.user--saved .name--detail {
  position: relative;
  left: 20px;
}
.user--saved .space {
  margin-top: 2%;
}
.detail--btn {
  position: relative;
  width: 20%;
  top: 30px;
  left: 37%;
  border-radius: 30px;

  color: #eee;
}
.link--color {
  color: white;
}
.foot {
  margin-top: 30%;
}
.hidden {
  display: none;
}
@media (min-width: 320px) and (max-width: 480px) {
  body {
    width: 100%;
    overflow-x: hidden;
  }
  .bg-image {
    overflow: hidden;
    height: 800px;
    width: 100%;
  }
  .navbar {
    width: 100%;
  }
  .first-content {
    display: inline-block;
    position: relative;
    top: 30px;
  }

  .HEADING {
    margin-top: 60px;
    margin-left: 20px;
    font-size: 100px;
  }
  .head {
    position: relative;
    display: inline;
    left: 1px;
    bottom: 5px;
  }
  .para {
    position: relative;

    width: 50%;
    top: 10px;
    left: 12px;
  }
  .para-1 {
    margin-top: 40px;
  }
  .bg-img-1 {
    height: 1000px;
    width: 100%;
  }
  .FORM-DIV {
    width: 75%;
    height: 850px;
    position: relative;
    bottom: 40px;
  }
  .sign-up {
    height: 70%;
  }

  .info {
    display: inline;
    padding: -20px;
    margin-top: -20px;
  }
  .FORM-DIV i {
    display: none;
  }
}

