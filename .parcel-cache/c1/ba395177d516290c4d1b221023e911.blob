  background-color: #ecf0f3;
  width: 100%;
}
.section--hidden {
  opacity: 0;
  transform: translateY(8rem);
}
.sticky {
  position: fixed;
  height: 100%;
  margin-top: -30px;
  width: 100%;
}
.navbar {
  display: flex;
  background: black;
}
.navbar-brand {
  font-family: "Times New Roman", Times, serif;
}
.collapse {
  justify-content: flex-end;
  margin-right: 20px;
}
.navbar-nav {
  font-family: "Abel", "sans-serif";
  font-size: x-large;
  font-weight: 450;
}
.nav-link {
  margin-left: 10px;
  color: #f0f5f5 !important;
}
.nav-link:hover {
  color: #e0ebeb !important;
}
.first-content {
  margin-top: 30px;
}
.HEADING {
  color: #4da6ff;
  font-size: 270px;
  margin-left: 23%;
  margin-top: 40px;
  font-family: "Abel", sans-serif;
  font-family: "Karantina", cursive;
  display: inline;
}
.head {
  color: #ffcc66;
  display: block;
  position: relative;
  bottom: 70px;
  left: 317px;
  font-size: 35px;
  font-family: "Abel", sans-serif;
  font-family: "Karantina", cursive;
}
.para {
  color: #1e1e15;
  display: inline;
  position: relative;
  left: 520px;
  bottom: 320px;
  text-align: justify;

  font-size: 10px;
  font-weight: lighter;
}
.h {
  color: #ffcc66;
}
.field {
  position: relative;
  top: 45px;
}
.text-muted {
  position: relative;
  top: 35px;
  font-size: 20px;
  color: white !important;
}
